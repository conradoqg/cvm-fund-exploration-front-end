{
  "name": "cvm-fund-explorer-front-end",
  "version": "1.20.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "budo ./js/index.js:build/bundle.js --live -P -- -d -g browserify-css -t babelify -t browserify-shim -t envify --full-paths",
    "build": "cross-env NODE_ENV=production  browserify ./js/index.js -g browserify-css -t babelify -t browserify-shim -t envify -g uglifyify | uglifyjs --compress --mangle > ./build/bundle.js",
    "check-build": "cross-env NODE_ENV=production browserify ./js/index.js -g browserify-css -t babelify -t browserify-shim -t envify -g uglifyify --full-paths --debug | uglifyjs --source-map \"content='inline'\" --compress --mangle -o ./build/check-bundle.js && discify ./build/check-bundle.js > out.html && source-map-explorer ./build/check-bundle.js ./build/check-bundle.js.map",
    "dist": "cp index.html ./build ./dist -r"
  },
  "browserify-shim": {
    "plotly": "global:Plotly"
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "."
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.1",
    "classnames": "^2.2.6",
    "d3-format": "^1.3.2",
    "dayjs": "^1.8.10",
    "immer": "^1.7.2",
    "mdi-material-ui": "^5.6.0",
    "promise-results": "^0.6.2",
    "rc-slider": "^8.6.3",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-plotly.js": "^2.2.0",
    "react-router-dom": "^4.4.0-beta.8"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "browserify-css": "^0.14.0",
    "browserify-shim": "^3.8.14",
    "budo": "^11.3.2",
    "cross-env": "^5.2.0",
    "disc": "^1.3.3",
    "envify": "^4.1.0",
    "source-map-explorer": "^1.6.0",
    "uglify-js": "^3.4.9",
    "uglifyify": "^5.0.1"
  }
}
